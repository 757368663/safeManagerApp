<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaopeng.safemanager">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.WelcomActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.SportActivity" />
        <activity android:name=".activity.SettingPersonInfoActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.SportDataActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.BackTableActivity" />
        <activity android:name=".activity.UsersManagerActivity" />
        <activity android:name=".activity.UpdateHealthDataActivity"></activity>
    </application>

</manifest>